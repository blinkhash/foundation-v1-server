{
  "name": "blinkhash-server",
  "version": "0.0.1",
  "description": "An extremely efficient, highly scalable, all-in-one, easy to setup cryptocurrency mining pool",
  "keywords": [
    "stratum",
    "mining",
    "pool",
    "server",
    "poolserver",
    "bitcoin",
    "litecoin",
    "scrypt"
  ],
  "homepage": "https://github.com/blinkhash/blinkhash-server",
  "bugs": {
    "url": "https://github.com/blinkhash/blinkhash-server/issues"
  },
  "license": "GPL-2.0",
  "author": {
    "name": "Nick Sarris"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "configs/",
      ".mock.js"
    ]
  },
  "scripts": {
    "start": "nodemon scripts/main.js",
    "start-v2": "nodemon scripts-v2/main.js",
    "clean": "eslint . --ext .js --ignore-pattern node_modules/",
    "clean:fix": "eslint . --ext .js --fix --ignore-pattern node_modules/",
    "test": "jest --coverage",
    "prod:start": "forever start -l blinkhash-log.txt --append scripts/init.js",
    "prod:stop": "forever stop scripts/init.js"
  },
  "contributors": [],
  "main": "scripts/init.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/blinkhash/blinkhash-server.git"
  },
  "dependencies": {
    "apicache": "^1.5.3",
    "async": "2.3.0",
    "bignum": "0.13.1",
    "blinkhash-stratum": "0.1.8",
    "body-parser": "*",
    "colors": "*",
    "compression": "*",
    "cors": "^2.8.5",
    "dateformat": "1.0.12",
    "dot": "*",
    "express": "*",
    "extend": "*",
    "forever": "^3.0.4",
    "mysql": "*",
    "node-watch": "0.5.9",
    "nodemon": "*",
    "nonce": "*",
    "redis": "^3.1.1",
    "request": "2.88.2"
  },
  "devDependencies": {
    "codecov": "^3.8.1",
    "eslint": "^7.23.0",
    "jest": "^26.6.3",
    "nock": "^13.0.10",
    "redis-mock": "^0.56.3"
  },
  "engines": {
    "node": ">=12.16"
  },
  "readmeFilename": "README.md"
}
